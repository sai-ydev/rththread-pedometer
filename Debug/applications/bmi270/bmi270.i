                    max_len = remain_len;
                }


                page_byte_idx = start_addr;
                while (page_byte_idx < max_len)
                {

                    lsb = (uint16_t) feat_config[page_byte_idx++];
                    if (page_byte_idx < max_len)
                    {
                        msb = ((uint16_t) feat_config[page_byte_idx++] << 8);
                    }

                    lsb_msb = lsb | msb;


                    step_count_params[param_idx] = lsb_msb & 0xFFFF;


                    param_idx++;
                }
            }
        }
