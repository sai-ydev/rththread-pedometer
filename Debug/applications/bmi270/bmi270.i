    uint16_t msb = 0;


    uint16_t lsb_msb = 0;


    struct bmi2_feature_config step_params_config = { 0, 0, 0 };


    uint8_t page_idx;


    uint8_t start_page;


    uint8_t start_addr;


    uint8_t n_bytes = (25 * 2);


    uint8_t n_pages = (n_bytes / 16);


    uint8_t end_page;


    uint8_t remain_len;


    uint8_t max_len = 16;


    uint8_t page_byte_idx;


    uint8_t param_idx = 0;


